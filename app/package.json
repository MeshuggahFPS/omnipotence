{
  "name": "omnipotence",
  "displayName": "Omnipotence",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "get-ready": "yarn install && cd ios && pod install && cd .. && node generateAppJson.js",
    "lint": "eslint --fix . && tsc --noEmit",
    "test": "JEST=true jest --coverage --silent --ci --coverageReporters=text --coverageReporters=cobertura --testResultsProcessor=jest-junit",
    "ios:dev": "react-native run-ios  --mode=Debug --scheme \"appDevelopment\"",
    "ios:test": "react-native run-ios --mode=Debug --scheme  \"appTest\"",
    "ios:prod": "react-native run-ios --mode=Release --scheme \"appProduction\"",
    "android:dev": "react-native run-android  --mode=Debug --scheme --mode=developmentdebug",
    "android:test": "react-native run-android --mode=Debug --scheme  --mode=testdebug",
    "android:prod": "react-native run-android --mode=Release --scheme --mode=productionrelease",
    "bump": "node ./semver.mjs",
    "clean": "rm -rf node_modules/ && rm -f yarn.lock && yarn cache clean && cd ios && rm -rf Pods/ && rm -rf build/ && rm -f Podfile.lock && cd .. && rm -rf android/build/ && rm -rf android/app/build/"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.17",
    "@types/styled-components-react-native": "^5.2.5",
    "axios": "^1.6.8",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-native": "0.73.7",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.13.1",
    "semver": "^7.6.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
